name="unfsd NFS server"
command=/usr/sbin/unfsd
pidfile=/var/run/unfsd.pid
lockfile=/var/lock/subsys/unfsd
command_args="-i $pidfile"

depend() {
	need net portmap
	after quota
	use ypbind dns
}
